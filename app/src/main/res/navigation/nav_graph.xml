<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/LoginFragment">

    <fragment
        android:id="@+id/HomeFragment"
        android:name="com.OnlineEvent.umangburman.event.HomeFragment.HomeFragment"
        android:label="First Fragment"
        tools:layout="@layout/first_fragment">
        <action
            android:id="@+id/action_Home_to_third"
            app:destination="@id/HomeFragment" />
        <action
            android:id="@+id/action_Home_to_About"
            app:destination="@id/AboutFragment" />
        <action
            android:id="@+id/action_Home_to_Schedule"
            app:destination="@id/SchduleFragment" />
        <action
            android:id="@+id/action_Home_to_Event"
            app:destination="@id/EventFragment" />
        <action
            android:id="@+id/action_Home_to_muyAccount"
            app:destination="@id/AccountFragment" />
    </fragment>

    <fragment
        android:id="@+id/secondFragment"
        android:name="com.OnlineEvent.umangburman.event.SecondFragment"
        android:label="Second Fragment"
        tools:layout="@layout/second_fragment"


        />
    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.OnlineEvent.umangburman.event.ThirdFragment"
        android:label="Third Fragment"

        tools:layout="@layout/third_fragment" />
    <fragment
        android:id="@+id/defaultFragment"
        android:name="com.OnlineEvent.umangburman.event.DefaultFragment"
        android:label="Default Fragment"
        tools:layout="@layout/default_fragment">

        <action
            android:id="@+id/action_login_to_home"
            app:destination="@id/HomeFragment" />

    </fragment>
    <fragment
        android:id="@+id/LoginFragment"
        android:name="com.OnlineEvent.umangburman.event.LoginFragment.LoginFragment"
        tools:layout="@layout/login_fragment">

        <action
            android:id="@+id/action_login_to_home"
            app:destination="@id/HomeFragment" />
        <action
            android:id="@+id/action_login_to_resetPassword"
            app:destination="@id/ResetPasswordFragment" />

    </fragment>
    <fragment
        android:id="@+id/AboutFragment"
        android:name="com.OnlineEvent.umangburman.event.AboutFragment.AboutFragment"
        tools:layout="@layout/about_fragment">


    </fragment>
    <fragment
        android:id="@+id/SchduleFragment"
        android:name="com.OnlineEvent.umangburman.event.SchduleFragment.ScheduleFragment"
        tools:layout="@layout/schdule_fragment">


    </fragment>
    <fragment
        android:id="@+id/EventFragment"
        android:name="com.OnlineEvent.umangburman.event.EventFragment.EventFragment"
        tools:layout="@layout/event_fragment">

        <action
            android:id="@+id/action_Event_ToDescription"
            app:destination="@id/EventDescriptionFragment" />
    </fragment>
    <fragment
        android:id="@+id/EventDescriptionFragment"
        android:name="com.OnlineEvent.umangburman.event.DescriptionFragment.DescriptionFragment"
        tools:layout="@layout/description_fragment">


    </fragment>
    <fragment
        android:id="@+id/AccountFragment"
        android:name="com.OnlineEvent.umangburman.event.AccountFragment.AccountFragment"
        tools:layout="@layout/my_account">


    </fragment>
    <fragment
        android:id="@+id/ResetPasswordFragment"
        android:name="com.OnlineEvent.umangburman.event.ResetPasswordFragment.ResetPasswordFragment"
        tools:layout="@layout/reset_password_fragment">


    </fragment>
    <activity
        android:id="@+id/secondActivity"
        android:name="com.OnlineEvent.umangburman.event.Main2Activity">

    </activity>
</navigation>